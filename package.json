{
  "name": "@elghandour/fetchify",
  "version": "1.0.1",
  "private": false,
  "description": "Universal wrapper for Fetch API",
  "main": "./dist/build.common.js",
  "module": "./dist/build.esm.js",
  "type": "commonjs",
  "types": "./dist/types/main.d.ts",
  "typings": "./dist/types/main.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmedElghandour1/fetchify.git"
  },
  "bugs": {
    "url": "https://github.com/ahmedElghandour1/fetchify/issues",
    "email": "georgian.stan8@gmail.com"
  },
  "homepage": "https://github.com/ahmedElghandour1/fetchify#readme",
  "sideEffects": false,
  "scripts": {
    "server": "nodemon ./server/index.js",
    "build": "cross-env NODE_ENV=production tsc --emitDeclarationOnly  && node ./esbuild.config.js",
    "watch:types": "tsc --emitDeclarationOnly --watch",
    "watch:ts": "cross-env WATCH=1 NODE_ENV=production  node ./esbuild.config.js",
    "watch": "concurrently  \"npm:watch:types\" \"npm:watch:ts\"",
    "build:wp": "cross-env NODE_ENV=production webpack --config webpack.config.js --mode production --env production",
    "watch:wp": "cross-env NODE_ENV=development webpack --watch --config webpack.config.js --mode development --env development",
    "prepublish": "npm run build",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build"
  },
  "exports": {
    ".": {
      "import": "./dist/build.esm.js",
      "require": "./dist/build.common.js"
    }
  },
  "author": "Ahmed Elghandour",
  "keywords": [
    "fetch",
    "fetchify",
    "browser",
    "node",
    "nodejs",
    "fetch wrapper",
    "uiversal fetch"
  ],
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.3",
    "node-abort-controller": "^3.0.1",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^17.0.32",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.38",
    "nodemon": "^2.0.15",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}